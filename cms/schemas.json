{
  "contentTypes": {
    "exam-course": {
      "kind": "collectionType",
      "collectionName": "exam_courses",
      "info": {
        "singularName": "exam-course",
        "pluralName": "exam-courses",
        "displayName": "Exam Course",
        "description": "Exam courses like UPSC, TNPSC, NEET etc."
      },
      "options": {
        "draftAndPublish": true
      },
      "attributes": {
        "title": {
          "type": "string",
          "required": true,
          "unique": true
        },
        "slug": {
          "type": "uid",
          "targetField": "title",
          "required": true
        },
        "summary": {
          "type": "text"
        },
        "order": {
          "type": "integer",
          "default": 1
        },
        "subjects": {
          "type": "relation",
          "relation": "oneToMany",
          "target": "api::subject.subject",
          "mappedBy": "exam_course"
        }
      }
    },
    "subject": {
      "kind": "collectionType",
      "collectionName": "subjects",
      "info": {
        "singularName": "subject",
        "pluralName": "subjects",
        "displayName": "Subject",
        "description": "Subjects within an exam course"
      },
      "options": {
        "draftAndPublish": true
      },
      "attributes": {
        "title": {
          "type": "string",
          "required": true,
          "unique": true
        },
        "slug": {
          "type": "uid",
          "targetField": "title",
          "required": true
        },
        "summary": {
          "type": "text"
        },
        "order": {
          "type": "integer",
          "default": 1
        },
        "exam_course": {
          "type": "relation",
          "relation": "manyToOne",
          "target": "api::exam-course.exam-course",
          "inversedBy": "subjects"
        },
        "units": {
          "type": "relation",
          "relation": "oneToMany",
          "target": "api::unit.unit",
          "mappedBy": "subject"
        }
      }
    },
    "unit": {
      "kind": "collectionType",
      "collectionName": "units",
      "info": {
        "singularName": "unit",
        "pluralName": "units",
        "displayName": "Unit",
        "description": "Units within a subject"
      },
      "options": {
        "draftAndPublish": true
      },
      "attributes": {
        "title": {
          "type": "string",
          "required": true,
          "unique": true
        },
        "slug": {
          "type": "uid",
          "targetField": "title",
          "required": true
        },
        "summary": {
          "type": "text"
        },
        "order": {
          "type": "integer",
          "default": 1
        },
        "subject": {
          "type": "relation",
          "relation": "manyToOne",
          "target": "api::subject.subject",
          "inversedBy": "units"
        },
        "chapters": {
          "type": "relation",
          "relation": "oneToMany",
          "target": "api::chapter.chapter",
          "mappedBy": "unit"
        }
      }
    },
    "chapter": {
      "kind": "collectionType",
      "collectionName": "chapters",
      "info": {
        "singularName": "chapter",
        "pluralName": "chapters",
        "displayName": "Chapter",
        "description": "Chapters within a unit"
      },
      "options": {
        "draftAndPublish": true
      },
      "attributes": {
        "title": {
          "type": "string",
          "required": true,
          "unique": true
        },
        "slug": {
          "type": "uid",
          "targetField": "title",
          "required": true
        },
        "summary": {
          "type": "text"
        },
        "order": {
          "type": "integer",
          "default": 1
        },
        "unit": {
          "type": "relation",
          "relation": "manyToOne",
          "target": "api::unit.unit",
          "inversedBy": "chapters"
        },
        "modules": {
          "type": "relation",
          "relation": "oneToMany",
          "target": "api::module.module",
          "mappedBy": "chapter"
        }
      }
    },
    "module": {
      "kind": "collectionType",
      "collectionName": "modules",
      "info": {
        "singularName": "module",
        "pluralName": "modules",
        "displayName": "Module",
        "description": "Modules within a chapter"
      },
      "options": {
        "draftAndPublish": true
      },
      "attributes": {
        "title": {
          "type": "string",
          "required": true,
          "unique": true
        },
        "slug": {
          "type": "uid",
          "targetField": "title",
          "required": true
        },
        "summary": {
          "type": "text"
        },
        "order": {
          "type": "integer",
          "default": 1
        },
        "chapter": {
          "type": "relation",
          "relation": "manyToOne",
          "target": "api::chapter.chapter",
          "inversedBy": "modules"
        },
        "contents": {
          "type": "relation",
          "relation": "oneToMany",
          "target": "api::content.content",
          "mappedBy": "module"
        }
      }
    },
    "content": {
      "kind": "collectionType",
      "collectionName": "contents",
      "info": {
        "singularName": "content",
        "pluralName": "contents",
        "displayName": "Content",
        "description": "Learning content within a module"
      },
      "options": {
        "draftAndPublish": true
      },
      "attributes": {
        "title": {
          "type": "string",
          "required": true
        },
        "slug": {
          "type": "uid",
          "targetField": "title",
          "required": true
        },
        "order": {
          "type": "integer",
          "default": 1
        },
        "body": {
          "type": "richtext",
          "required": true
        },
        "module": {
          "type": "relation",
          "relation": "manyToOne",
          "target": "api::module.module",
          "inversedBy": "contents"
        }
      }
    },
    "student": {
      "kind": "collectionType",
      "collectionName": "students",
      "info": {
        "singularName": "student",
        "pluralName": "students",
        "displayName": "Student",
        "description": "Student profiles and information"
      },
      "options": {
        "draftAndPublish": false
      },
      "attributes": {
        "name": {
          "type": "string",
          "required": true
        },
        "email": {
          "type": "email",
          "required": true,
          "unique": true
        },
        "mobile": {
          "type": "string"
        },
        "dob": {
          "type": "date"
        },
        "state": {
          "type": "string"
        },
        "district": {
          "type": "string"
        },
        "exam": {
          "type": "enumeration",
          "enum": ["UPSC", "TNPSC", "NEET"],
          "required": true
        },
        "targetYear": {
          "type": "integer"
        },
        "otherInfo": {
          "type": "text"
        },
        "user": {
          "type": "relation",
          "relation": "oneToOne",
          "target": "plugin::users-permissions.user"
        },
        "enrollments": {
          "type": "relation",
          "relation": "oneToMany",
          "target": "api::enrollment.enrollment",
          "mappedBy": "student"
        },
        "studySessions": {
          "type": "relation",
          "relation": "oneToMany",
          "target": "api::study-session.study-session",
          "mappedBy": "student"
        },
        "todos": {
          "type": "relation",
          "relation": "oneToMany",
          "target": "api::todo.todo",
          "mappedBy": "student"
        },
        "progresses": {
          "type": "relation",
          "relation": "oneToMany",
          "target": "api::progress.progress",
          "mappedBy": "student"
        }
      }
    },
    "enrollment": {
      "kind": "collectionType",
      "collectionName": "enrollments",
      "info": {
        "singularName": "enrollment",
        "pluralName": "enrollments",
        "displayName": "Enrollment",
        "description": "Student enrollments in exam courses"
      },
      "options": {
        "draftAndPublish": false
      },
      "attributes": {
        "student": {
          "type": "relation",
          "relation": "manyToOne",
          "target": "api::student.student",
          "inversedBy": "enrollments"
        },
        "exam_course": {
          "type": "relation",
          "relation": "manyToOne",
          "target": "api::exam-course.exam-course"
        },
        "enrolledAt": {
          "type": "datetime",
          "default": "now()"
        },
        "status": {
          "type": "enumeration",
          "enum": ["active", "completed", "paused", "cancelled"],
          "default": "active"
        }
      }
    },
    "study-session": {
      "kind": "collectionType",
      "collectionName": "study_sessions",
      "info": {
        "singularName": "study-session",
        "pluralName": "study-sessions",
        "displayName": "Study Session",
        "description": "Student study sessions tracking"
      },
      "options": {
        "draftAndPublish": false
      },
      "attributes": {
        "student": {
          "type": "relation",
          "relation": "manyToOne",
          "target": "api::student.student",
          "inversedBy": "studySessions"
        },
        "date": {
          "type": "date",
          "required": true
        },
        "startAt": {
          "type": "time",
          "required": true
        },
        "endAt": {
          "type": "time",
          "required": true
        },
        "minutes": {
          "type": "integer",
          "required": true
        },
        "notes": {
          "type": "text"
        }
      }
    },
    "todo": {
      "kind": "collectionType",
      "collectionName": "todos",
      "info": {
        "singularName": "todo",
        "pluralName": "todos",
        "displayName": "Todo",
        "description": "Student todo items and tasks"
      },
      "options": {
        "draftAndPublish": false
      },
      "attributes": {
        "student": {
          "type": "relation",
          "relation": "manyToOne",
          "target": "api::student.student",
          "inversedBy": "todos"
        },
        "title": {
          "type": "string",
          "required": true
        },
        "description": {
          "type": "text"
        },
        "dueDate": {
          "type": "datetime"
        },
        "status": {
          "type": "enumeration",
          "enum": ["open", "in-progress", "done"],
          "default": "open"
        },
        "priority": {
          "type": "enumeration",
          "enum": ["low", "medium", "high"],
          "default": "medium"
        }
      }
    },
    "progress": {
      "kind": "collectionType",
      "collectionName": "progresses",
      "info": {
        "singularName": "progress",
        "pluralName": "progresses",
        "displayName": "Progress",
        "description": "Student progress tracking for content"
      },
      "options": {
        "draftAndPublish": false
      },
      "attributes": {
        "student": {
          "type": "relation",
          "relation": "manyToOne",
          "target": "api::student.student",
          "inversedBy": "progresses"
        },
        "content": {
          "type": "relation",
          "relation": "manyToOne",
          "target": "api::content.content"
        },
        "percent": {
          "type": "decimal",
          "min": 0,
          "max": 100,
          "default": 0
        },
        "status": {
          "type": "enumeration",
          "enum": ["not_started", "in_progress", "done"],
          "default": "not_started"
        },
        "lastAccessedAt": {
          "type": "datetime"
        },
        "completedAt": {
          "type": "datetime"
        }
      }
    }
  },
  "permissions": {
    "public": {
      "description": "Public access to content types",
      "allowedActions": ["find", "findOne"],
      "contentTypes": [
        "api::exam-course.exam-course",
        "api::subject.subject",
        "api::unit.unit",
        "api::chapter.chapter",
        "api::module.module",
        "api::content.content"
      ]
    },
    "authenticated": {
      "description": "Authenticated user access to student data",
      "allowedActions": ["find", "findOne", "create", "update", "delete"],
      "contentTypes": [
        "api::student.student",
        "api::enrollment.enrollment",
        "api::study-session.study-session",
        "api::todo.todo",
        "api::progress.progress"
      ]
    }
  },
  "apiEndpoints": {
    "public": [
      "GET /api/exam-courses",
      "GET /api/exam-courses/:id",
      "GET /api/subjects",
      "GET /api/subjects/:id",
      "GET /api/units",
      "GET /api/units/:id",
      "GET /api/chapters",
      "GET /api/chapters/:id",
      "GET /api/modules",
      "GET /api/modules/:id",
      "GET /api/contents",
      "GET /api/contents/:id"
    ],
    "authentication": [
      "POST /api/auth/local/register",
      "POST /api/auth/local",
      "POST /api/auth/logout",
      "POST /api/auth/forgot-password",
      "POST /api/auth/reset-password",
      "GET /api/auth/me"
    ],
    "authenticated": [
      "GET /api/students",
      "POST /api/students",
      "PUT /api/students/:id",
      "GET /api/enrollments",
      "POST /api/enrollments",
      "GET /api/study-sessions",
      "POST /api/study-sessions",
      "GET /api/todos",
      "POST /api/todos",
      "GET /api/progresses",
      "POST /api/progresses"
    ]
  }
}
