{
  "kind": "collectionType",
  "collectionName": "subjects",
  "info": {
    "singularName": "subject",
    "pluralName": "subjects",
    "displayName": "Subject",
    "description": "Subjects within an exam course"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "summary": {
      "type": "text"
    },
    "order": {
      "type": "integer",
      "default": 1
    },
    "exam_course": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::exam-course.exam-course",
      "inversedBy": "subjects"
    },
    "units": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::unit.unit",
      "mappedBy": "subject"
    }
  }
}
