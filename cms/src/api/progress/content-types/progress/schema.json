{
  "kind": "collectionType",
  "collectionName": "progresses",
  "info": {
    "singularName": "progress",
    "pluralName": "progresses",
    "displayName": "Progress",
    "description": "Student progress tracking for content"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "student": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::student.student",
      "inversedBy": "progresses"
    },
    "content": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::content.content"
    },
    "percent": {
      "type": "decimal",
      "min": 0,
      "max": 100,
      "default": 0
    },
    "status": {
      "type": "enumeration",
      "enum": ["not_started", "in_progress", "done"],
      "default": "not_started"
    },
    "lastAccessedAt": {
      "type": "datetime"
    },
    "completedAt": {
      "type": "datetime"
    }
  }
}
