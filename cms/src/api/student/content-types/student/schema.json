{
  "kind": "collectionType",
  "collectionName": "students",
  "info": {
    "singularName": "student",
    "pluralName": "students",
    "displayName": "Student",
    "description": "Student profiles and information"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "email": {
      "type": "email",
      "required": true,
      "unique": true
    },
    "mobile": {
      "type": "string"
    },
    "dob": {
      "type": "date"
    },
    "state": {
      "type": "string"
    },
    "district": {
      "type": "string"
    },
    "exam": {
      "type": "enumeration",
      "enum": ["UPSC", "TNPSC", "NEET"],
      "required": true
    },
    "targetYear": {
      "type": "integer"
    },
    "otherInfo": {
      "type": "text"
    },
    "user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "enrollments": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::enrollment.enrollment",
      "mappedBy": "student"
    },
    "studySessions": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::study-session.study-session",
      "mappedBy": "student"
    },
    "todos": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::todo.todo",
      "mappedBy": "student"
    },
    "progresses": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::progress.progress",
      "mappedBy": "student"
    }
  }
}
